function preloadNexFragment(a,b){if(Video.isMobile.any()){if(b>=Math.floor(a.duration()/4))return!1;var d=a.src();""==a.getAttribute("data-mode")?d=d.replace("/vne/master.m3u8","/seg-0-v1-a1.ts").replace("/index-v1-a1.m3u8","/seg-0-v1-a1.ts"):(d=d.replace(/,240p|,360p|,480p/gi,"").replace(",,","").replace("mp4//","mp4/").replace("/vne/master.m3u8","/seg-0-v1-a1.ts").replace("/index-v1-a1.m3u8","/seg-0-v1-a1.ts"),a.videoHeight()!=a.maxMode&&d.replace("/mp4/","/mp4/"+a.videoHeight()+"p/"));d=d.replace("-0-",
