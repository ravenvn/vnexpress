function checkBuffer(a){switch(a.videoHeight()){case 240:a.maxMaxBufferLengthVal=30;break;case 360:a.maxMaxBufferLengthVal=20;break;case 480:a.maxMaxBufferLengthVal=16;case 720:a.maxMaxBufferLengthVal=12}if(!Video.isMobile.any())try{a.tech_.sourceHandler_.hls.config.maxMaxBufferLength=a.maxMaxBufferLengthVal,Math.floor(a.bufferedEnd()-a.currentTime())>=a.tech_.sourceHandler_.hls.config.maxMaxBufferLength&&a.currentTime()>a.duration()/2&&(a.maxMaxBufferLengthVal=a.tech_.sourceHandler_.hls.config.maxMaxBufferLength=
a.duration()-a.currentTime())}catch(b){console.log("not using hls plugins!!!")}if(1>a.duration()-a.currentTime())return!1;a.paused()?a.stopBuff&&(2<a.currentTime()&&Math.floor(a.bufferedEnd()-a.currentTime())>=a.maxMaxBufferLengthMinVal||Math.floor(a.bufferedEnd()-a.currentTime())>=a.maxMaxBufferLengthMaxVal||a.duration()-a.currentTime()<a.maxMaxBufferLengthMinVal)&&(a.firstBuff=!1,a.stopBuff=!1,a.play(),a.removeClass("vjs-waiting"),a.removeClass("vjs-controls-disabled"),a.addClass("vjs-controls-enabled")):
a.firstBuff?2<a.currentTime()&&Math.floor(a.bufferedEnd()-a.currentTime())<a.maxMaxBufferLengthMinVal&&(a.stopBuff=!0,a.pause(),a.removeClass("vjs-controls-enabled"),a.addClass("vjs-controls-disabled"),setTimeout(function(){a.removeClass("vjs-paused");a.addClass("vjs-waiting")},100)):0>=a.bufferedEnd()-a.currentTime()&&(a.stopBuff=!0,a.pause(),a.removeClass("vjs-controls-enabled"),a.addClass("vjs-controls-disabled"),setTimeout(function(){a.removeClass("vjs-paused");a.addClass("vjs-waiting")},100))}
function preloadNexFragmentCallBack(a){a.paused()&&(3<a["preloadTS"+a.videoHeight()].length?6+4*(a["preloadTS"+a.videoHeight()].length-3):2*a["preloadTS"+a.videoHeight()].length)<a.currentTime()+2*a.maxMaxBufferLengthVal&&preloadNexFragment(a,a["preloadTS"+a.videoHeight()].length)}
